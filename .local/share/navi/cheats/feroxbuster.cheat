% feroxbuster

# non-recursive forced browsing
feroxbuster -u <URL> -w /usr/share/seclists/Discovery/Web-Content/common.txt -q -n -o ferox.txt

# filter out status code
feroxbuster -u <URL> -w /usr/share/seclists/Discovery/Web-Content/common.txt -C <STATUS_CODE> -q -n -o ferox.txt

# limit connections THREAD*CON_LIMIT (avoid DoS)
feroxbuster -u <URL> -w /usr/share/seclists/Discovery/Web-Content/common.txt -t <THREADS> -L <CON_LIMIT> -q -n -o ferox.txt
